{
    "name": "Faceit X steam",
    "version": "1.0.1",
    "manifest_version": 3,
    "description": "Chrome extension to retrieve faceit stats from steam profile",
    "content_scripts": [
      {
        "matches": ["https://steamcommunity.com/id/*", "https://steamcommunity.com/profiles/*"],
        "js": ["content.js"]
      }
    ],
    "icons": {
      "16": "images/ExtensionImage/FaceitXsteam.png",
      "48": "images/ExtensionImage/FaceitXsteam.png",
      "128": "images/ExtensionImage/FaceitXsteam.png"
    },
    "web_accessible_resources": [
      {
        "resources": [
                      "images/faceit1.svg",
                      "images/faceit2.svg",
                      "images/faceit3.svg",
                      "images/faceit4.svg",
                      "images/faceit5.svg",
                      "images/faceit6.svg",
                      "images/faceit7.svg",
                      "images/faceit8.svg",
                      "images/faceit9.svg",
                      "images/faceit10.svg"
                    ],
        "matches": ["<all_urls>"]
      }
    ]
  }